 int funkcjaA()
    {
    float liczba1;
    float liczba2;
    float wynik;
    printf("Podaj Pierwsza liczbe:\n");
    scanf("%f", &liczba1);
    printf("Podaj Druga Liczbe:\n");
    scanf("%f", &liczba2);
    wynik = liczba1 * liczba2;
    printf("%f * %f = %f\n", liczba1, liczba2, wynik);
    }
